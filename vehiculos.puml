@startuml clases

class Vehiculo {
  - color: String
  - ruedas: Int
  - numBastidor: String
}

class Coche {
  - velocidad: Int
  - cilindrada: Int
}

class Bicicleta {
  - tipo: String
}

class Formula1 {
  - equipo: String
}

class Camioneta {
  - carga: Int
}

class Motocicleta {
  - velocidad: Int
  - cilindrada: Int
}

class Quad {
  - modelo: String
  - carga: Int
}

Vehiculo --|> Coche
Vehiculo --|> Bicicleta
Coche --|> Formula1
Coche --|> Camioneta
Bicicleta --|> Motocicleta
Coche --|> Quad
Bicicleta --|> Quad

@enduml
